{
  "env": {
    "browser": false,
    "node": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/array-type": ["error", "generic"],
    "@typescript-eslint/indent": ["error", "tab"],
    "@typescript-eslint/member-ordering": "error",
    "@typescript-eslint/no-non-null-assertion": "error"
  }
}
